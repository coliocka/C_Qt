/*首页左侧的列表组件*/
QScrollArea#scrollArea
{
    background-color:rgb(255,255,255);
    border:0px;
    border-right:1px solid rgb(205,205,205);
}

QWidget#scrollAreaWidgetContents
{
    background-color:rgb(255,255,255);
}

QListWidget#listT1,#listT2,#listT3
{
    padding-top:5px;
    border:0px;
    color:rgb(55,55,55);
    background-color:rgb(255,255,255);
    padding-left:15px;
}

QListWidget::Item#listT1,::Item#listT2,::Item#listT3
{
    height:36;
    border:0px solid rgb(255,255,255);
    padding-top:1px;
}

QListWidget::Item:hover#listT1,::Item:hover#listT2,::Item:hover#listT3
{
    background-color:rgb(246,246,247);
}

QListWidget::Item:selected#listT1,::Item:selected#listT2,::Item:selected#listT3
{
    font-size:16px;
    font-weight: bold;
    color:rgb(55,55,55);
    border:0px;
    background-color:rgb(246,246,247);
}

QLabel#myMusicLabel,#createSonglistLabel
{
    padding-left:15px;
    background-color:rgb(255,255,255);
    color:rgb(159,159,159);
}

QPushButton#addButton
{
    image:url(images/homeList/加.png);
    background-color:rgb(255,255,255);
}

QPushButton:hover#addButton
{
    background-color:rgb(246,246,247);
}


/*如果不去轮廓线,item被选中的有问题*/
*
{
    outline:0px;
}



/*首页右侧TabWidget*/
QTabWidget::pane
{
    top:10px;
    border:0px;
}

QTabBar::tab
{
    background-color:rgb(255,255,255);
    border-bottom:3px solid rgb(255,255,255);
    font-family:"微软雅黑";
    font-size:16px;
    padding-left:5px;
    padding-right:5px;
    width:85px;
    height:30px;
}

QTabBar::tab:hover
{
    border-bottom:3px solid rgb(236,65,65);
    color:rgb(55,55,55);
    font-size:20px;
    font-weight:bold;
}


QTabBar::tab:selected
{
    border-bottom:3px solid rgb(236,65,65);
    color:rgb(55,55,55);
    font-size:20px;
    font-weight:bold;
}



