/*音乐播放条*/
QSlider::add-page:Horizontal#musicProgressSlider
{
        background-color: rgb(205,205,205);height:4px;
}
QSlider::sub-page:Horizontal#musicProgressSlider
{
        background-color: rgb(255,78,78);height:4px;
}
QSlider::handle:Horizontal#musicProgressSlider
{
        width:14px;image: url(images/tail/音乐进度点.png);margin:-4 px -4px;
}
QSlider::groove:Horizontal#musicProgressSlider
{
        background:rgb(0, 255, 0);height:4px;
}

